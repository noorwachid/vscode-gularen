{
  "name": "gularen",
  "displayName": "Gularen",
  "description": "Gularen markup language support for VS Code",
  "version": "0.4.0",
  "publisher": "nwachid",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gularen",
        "aliases": [
          "Gularen",
          "gularen"
        ],
        "extensions": [
          ".gr",
          ".gularen"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gularen",
        "scopeName": "source.gularen",
        "path": "./syntaxes/gularen.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "gularen.preview",
        "title": "Preview"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == gularen",
          "command": "gularen.preview",
          "group": "navigation"
        }
      ]
    }
  },
  "main": "./extension.js",
  "devDependencies": {
    "eslint": "^8.56.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
